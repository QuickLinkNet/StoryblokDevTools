{
  "name": "storyblok-dev-tools",
  "version": "1.0.19",
  "description": "Advanced developer tools overlay for Storyblok CMS in Next.js applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "npm run build:tailwind && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "build:tailwind": "npx tailwindcss -i ./components/storyblok/tailwind-devtools.css -o ./components/storyblok/tailwind-compiled.css --minify && node scripts/embed-tailwind.js",
    "build:package": "npm run build:tailwind && rm -rf dist && tsc --project tsconfig.build.json",
    "prepublishOnly": "npm run build:package"
  },
  "keywords": [
    "storyblok",
    "cms",
    "nextjs",
    "developer-tools",
    "devtools",
    "debug",
    "overlay",
    "inspector"
  ],
  "author": "QuickLink",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/QuickLinkNet/StoryblokDevTools.git"
  },
  "bugs": {
    "url": "https://github.com/QuickLinkNet/StoryblokDevTools/issues"
  },
  "homepage": "https://github.com/QuickLinkNet/StoryblokDevTools",
  "peerDependencies": {
    "next": "^13.5.0 || ^14.0.0 || ^15.0.0",
    "react": ">=18.2.0 <20",
    "react-dom": ">=18.2.0 <20"
  },
  "dependencies": {
    "@storyblok/react": "^5.4.13",
    "lucide-react": "^0.446.0"
  },
  "devDependencies": {
    "@types/node": "20.6.2",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "eslint": "8.49.0",
    "eslint-config-next": "13.5.1",
    "next": "14.2.13",
    "postcss": "8.4.30",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "5.2.2"
  }
}
